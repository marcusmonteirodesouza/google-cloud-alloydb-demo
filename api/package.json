{
  "name": "google-cloud-alloy-db-demo-app-api",
  "version": "1.0.0",
  "description": "Google Cloud AlloyDB demo app API",
  "private": true,
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "files": [
    "build/src"
  ],
  "license": "",
  "keywords": [],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "format": "prettier --write .",
    "dev": "ts-node-dev -r dotenv-expand/config src/index.ts",
    "start": "docker compose up --build",
    "database:up": "docker compose up database --detach --wait",
    "down": "docker compose down",
    "knex": "knex",
    "migrate:latest": "knex migrate:latest",
    "migrate:rollback": "knex migrate:rollback",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run lint"
  },
  "devDependencies": {
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.17",
    "@types/node": "20.4.6",
    "gts": "^5.0.1",
    "prettier": "^3.0.3",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "~5.1.6"
  },
  "dependencies": {
    "@relaycorp/pino-cloud": "^1.0.28",
    "celebrate": "^15.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "dotenv-expand": "^10.0.0",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "http-status-codes": "^2.3.0",
    "knex": "^2.5.1",
    "knex-stringcase": "^1.4.6",
    "pg": "^8.11.3",
    "pino": "^8.15.1",
    "pino-http": "^8.5.0"
  }
}
